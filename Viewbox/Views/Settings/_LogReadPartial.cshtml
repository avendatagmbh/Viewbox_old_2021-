@using SystemDb
@using Viewbox
@using Viewbox.Properties
@model Viewbox.Models.LogReadModel

<div id="rightsettings">
    <h3 class="users">@Resources.FileContent</h3>
    <div class="read-content">
     @{
             string[] texts = Model.text.ToString().Split(new string[] { "\r\n" }, StringSplitOptions.None);
             for (int i = 0; i < texts.Length; i++)
             {
              <p style="padding-bottom: 0px; white-space: nowrap;">@texts[i]</p>
             }
         
     }
    </div>
</div>