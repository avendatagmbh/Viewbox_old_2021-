@using Viewbox.Properties
@model Viewbox.Models.DeadUrlModel
@{ ViewBag.Title = Resources.DeadUrlCaption; }
<div id="url-dlg" class="hide">
    @Html.Partial("_DialogPartial", Model.Dialog)
</div>
<script type="text/javascript" language="javascript">
    $(document).ready(function() {
        jQuery('#dialog').show().html(jQuery('#url-dlg').html()).addClass("dead-url");
    });
</script>