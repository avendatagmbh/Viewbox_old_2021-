<Window x:Class="eBalanceKit.Controls.BalanceList.BalanceListViewErrors"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Localisation="clr-namespace:eBalanceKitResources.Localisation;assembly=eBalanceKitResources" 
        Title="{x:Static Localisation:ResourcesCommon.ViewImportErrors}" Icon="/CustomResources;component/Resources/logo1.png" Height="451" Width="757"
        xmlns:my="clr-namespace:eBalanceKit.Controls.BalanceList"
        MinWidth="400" MinHeight="200"
        xmlns:AvdWpfControls="clr-namespace:AvdWpfControls;assembly=AvdWpfControls">
    <Grid>
        <AvdWpfControls:AssistantControl
            CancelButtonVisibility="Collapsed"
            IsNavigationButtonVisible="False"
            Name="assistantControl"
            ShowSteps="False">
            <AvdWpfControls:AssistantTabItem Header="{x:Static Localisation:ResourcesCommon.ViewImportErrors}">
                <my:BalListImpAssistViewErrors x:Name="viewErrorsCtl" />
            </AvdWpfControls:AssistantTabItem>
        </AvdWpfControls:AssistantControl>
    </Grid>
</Window>
