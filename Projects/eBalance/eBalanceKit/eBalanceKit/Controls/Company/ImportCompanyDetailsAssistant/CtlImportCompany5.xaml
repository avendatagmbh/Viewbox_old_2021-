<UserControl x:Class="eBalanceKit.Controls.Company.ImportCompanyDetailsAssistant.CtlImportCompany5"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Localisation="clr-namespace:eBalanceKitResources.Localisation;assembly=eBalanceKitResources"
             mc:Ignorable="d" 
             d:DesignHeight="430" d:DesignWidth="750">
    <Grid>
        <StackPanel Height="Auto" Width="Auto">
            <Label Content="{x:Static Localisation:ResourcesCompanyImport.Summary}" Height="28" Name="label1" />
