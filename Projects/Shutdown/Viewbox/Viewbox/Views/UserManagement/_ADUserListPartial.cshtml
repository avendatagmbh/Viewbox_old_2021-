@using Viewbox.Properties

<div class="sortable-users sortable">
    <div class="droparea">
        @foreach (var user in Model)
        {
            <div class="user">
                <div class="domain">@user.Item3</div>
                <div class="name">@user.Item1</div>
                <div class="kuerzel">(@user.Item2)</div>
            </div>
        }
    </div>    
</div>