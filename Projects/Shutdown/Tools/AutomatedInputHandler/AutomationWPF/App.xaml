<Application x:Class="AutomationWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Windows\DlgMain.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AvdWpfControls;component/themes/BlueDesign.xaml" />
                <ResourceDictionary Source="pack://application:,,,/AvdWpfStyles;component/Default.xaml" />
                <ResourceDictionary Source="pack://application:,,,/AvdWpfStyles;component/BlueDesignResources.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/AppResources.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/NavigationResources.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/ImplicitStylesResourceDictionary.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/IconsResourceDictionary.xaml" />
                <ResourceDictionary Source="ResourceDictionaries/StylesResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
